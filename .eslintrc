{
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
  },
  "globals": {
    "window": true,
    "fetch": true,
    "Request": true,
    "document": true,
    "console": true,
    "module": true,
    "API_HOST": true,
    "localStorage": true,
    "GetVendorList": true,
    "GetStoreLookups": true,
    "$": true,
    "setTimeout": true,
    "GetGoogleSummary": true,
    "GetGoogleItems": true
  }
}
